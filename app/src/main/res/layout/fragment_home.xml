<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearHomepage"
        android:layout_height="match_parent"

        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_300_dd"
        tools:context=".ui.fragments.HomeFragment">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnmenu"
            android:layout_height="match_parent"


            android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top|start"
                android:layout_marginBottom="@dimen/_10pxv"
                app:contentInsetStart="0dp">

                <LinearLayout android:id="@+id/linearNavbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView
                    android:id="@+id/logo"
                    android:layout_width="154dp"
                    android:layout_height="62dp"

                    android:layout_gravity="left"
                    android:layout_marginStart="@dimen/_20pxh"

                    android:src="@drawable/logoo" />

                </LinearLayout>


            </androidx.appcompat.widget.Toolbar>
            <FrameLayout android:id="@+id/frameStackmenu"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_199pxv"
                 android:layout_width="@dimen/_400pxh">


                <FrameLayout android:id="@+id/frameBanner"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_200pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                    <FrameLayout android:id="@+id/frameBackground"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_166pxv"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" style="@style/groupStyleindigo_300cornerRadius">
                        <ImageView android:id="@+id/imagePattern"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_165pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription" android:src="@drawable/img_pattern"/>
                    </FrameLayout>
                    <ImageView android:id="@+id/imageImageOne"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_195pxh"
                        android:layout_marginEnd="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_width="@dimen/_135pxh"
                        android:scaleType="fitXY"

                        tools:ignore="ContentDescription" android:src="@drawable/img_image"/>
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtRservezetpla"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_44pxv"
                        android:layout_marginEnd="@dimen/_18pxh"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_44pxv"
                        android:layout_width="180dp"
                        android:text="@string/msg_r_servez_et_pla"
                        style="@style/txtIntermedium17" />
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnConsulter"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_37pxv"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="@dimen/_108pxh"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_26pxh"
                        android:paddingStart="@dimen/_26pxh"
                        android:paddingTop="@dimen/_11pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_consulter"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_consulter"/>
                </FrameLayout>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearCard"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearGeneral"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_weight="0.25"
                    android:layout_width="0dp" android:orientation="vertical">
                    <FrameLayout android:id="@+id/frameIcon"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_64pxv"
                        android:layout_width="@dimen/_70pxh" style="@style/groupStylegray_50cornerRadius">
                        <ImageView android:id="@+id/imageManDoctorEuro"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_33pxh"
                            android:layout_marginHorizontal="@dimen/_15pxh"
                            android:layout_marginVertical="@dimen/_15pxv"
                            android:layout_width="@dimen/_39pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_mandoctoreuro"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_mandoctoreuro"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtMdecins"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_7pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/lbl_m_decins"
                        style="@style/txtInterregular11" />
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearNeurologic"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_weight="0.24"
                    android:layout_width="0dp" android:orientation="vertical">
                    <FrameLayout android:id="@+id/frameIcon1"
                        android:layout_height="@dimen/_64pxv"
                        android:layout_width="@dimen/_68pxh" style="@style/groupStylegray_50cornerRadius">
                        <ImageView android:id="@+id/imageBrainOne"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_33pxh"
                            android:layout_marginHorizontal="@dimen/_15pxh"
                            android:layout_marginVertical="@dimen/_15pxv"
                            android:layout_width="@dimen/_36pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_brain1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_brain1"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtPharmacies"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/lbl_pharmacies"
                        style="@style/txtInterregular11" />
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearPediatric"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_weight="0.27"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_gray_50_radius_32_4"
                        android:id="@+id/btnIcon"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_64pxh"
                        android:layout_marginEnd="@dimen/_5pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="@dimen/_64pxh"
                        android:paddingHorizontal="@dimen/_15pxh"
                        android:paddingVertical="@dimen/_15pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_icon" tools:ignore="ContentDescription"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtMedicaments"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/lbl_medicaments"
                        style="@style/txtInterregular11" />
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRadiology"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_4pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_weight="0.23"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_gray_50_radius_32_4"
                        android:id="@+id/btnIconOne"
                        android:layout_height="@dimen/_64pxh"
                        android:layout_width="@dimen/_64pxh"
                        android:paddingHorizontal="@dimen/_15pxh"
                        android:paddingVertical="@dimen/_15pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_icon_64x64" tools:ignore="ContentDescription"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtQuestions"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/lbl_questions"
                        style="@style/txtInterregular11" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowspcialits"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSpcialits"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.71"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_sp_cialit_s"
                    style="@style/txtIntermedium17_1" />
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSeeAll"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_208pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.29"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_see_all"
                    style="@style/txtInterregular11_1" />
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>
</layout>
